	keywords = {SEMI-READ, Bandwidth, Hardware, inter-active media, Media, point cloud, Quality of experience, Rendering (computer graphics), streaming, Streaming media, Three-dimensional displays, volumetric video},
	pages = {191--199},
	file = {Full Text PDF:/home/georgefkd/Zotero/storage/U3AV5L8U/Alkhalili et al. - 2020 - A Survey of Volumetric Content Streaming Approaches.pdf:application/pdf},
